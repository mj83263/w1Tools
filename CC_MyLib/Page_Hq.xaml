<Page x:Class="CC_MyLib.Page_Hq"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:CC_MyLib"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page_Hq"
      Name="mySelf"
      >
    <Page.Resources>
    </Page.Resources>
    <DockPanel LastChildFill="True">
        <Grid Height="120" DockPanel.Dock="Bottom" Background="AliceBlue">
            
        </Grid>
        <Grid Background="AntiqueWhite">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"></ColumnDefinition>
                <ColumnDefinition Width="60"></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="*"></RowDefinition>
                <RowDefinition Height="60"></RowDefinition>
            </Grid.RowDefinitions>
            <Grid Grid.Row="0" Grid.Column="1" Grid.RowSpan="2" Background="Aqua"></Grid>
            <Grid Grid.Row="1" Grid.Column="0" Background="Aqua"></Grid>

            <!--<Path Data="m1,8 h0.8 v-6 h0.2 v6 h0.8 v2 h-0.8 v5 h-0.2 v-5 h-0.8 v-2" Fill="Black"></Path>-->
        </Grid>
    </DockPanel>
</Page>
