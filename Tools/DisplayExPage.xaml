<Page x:Class="Tools.DisplayExPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Tools"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      xmlns:ScottPlot="clr-namespace:ScottPlot.WPF;assembly=ScottPlot.WPF"
      Title="DisplayExPage">
    <Page.CommandBindings>
        <CommandBinding Command="{x:Static NavigationCommands.PreviousPage}" CanExecute="CanExecutePreviousPage" Executed="PreviousPage_Executed"/>
        <CommandBinding Command="{x:Static NavigationCommands.NextPage}" CanExecute="CanExecuteNextPage" Executed="NextPage_Executed"/>
    </Page.CommandBindings>
    <Page.InputBindings>
        <KeyBinding Key="Left" Command="{x:Static NavigationCommands.PreviousPage}"/>
        <KeyBinding Key="Right" Command="{x:Static NavigationCommands.NextPage}" />
    </Page.InputBindings>
    <Grid >
        <ScottPlot:WpfPlot x:Name="Plot1" >
            
        </ScottPlot:WpfPlot>
    </Grid>
</Page>
